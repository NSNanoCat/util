{
  "name": "@nsnanocat/util",
  "description": "Pure JS's util module for well-known iOS network tools",
  "author": "VirgilClyne <Virgil@nanocat.me>",
  "homepage": "https://NSNanoCat.github.io/util",
  "keywords": ["loon", "quantumult", "surge", "shadowrocket", "stash", "egern"],
  "license": "Apache-2.0",
  "bugs": "https://github.com/NSNanoCat/util/issues",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./getStorage": {
      "types": "./dist/getStorage.d.ts",
      "import": "./dist/getStorage.mjs"
    },
    "./polyfill/**": {
      "types": "./dist/polyfill/**.d.ts",
      "import": "./dist/polyfill/**.mjs"
    },
    "./lib/**": {
      "types": "./dist/lib/**.d.ts",
      "import": "./dist/lib/**.mjs"
    }
  },
  "scripts": {
    "build": "rslib build",
    "check": "biome check --write",
    "dev": "rslib build --watch",
    "format": "biome format --write",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NSNanoCat/util.git"
  },
  "files": ["dist"],
  "dependencies": {
    "type-fest": "^4.31.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.2",
    "@rslib/core": "^0.2.2",
    "@types/node": "^22.10.2",
    "fetch-cookie": "^3.1.0",
    "node-fetch": "^3.3.2",
    "typescript": "^5.6.3"
  },
  "optionalDependencies": {
    "fetch-cookie": "^3.1.0",
    "node-fetch": "^3.3.2"
  }
}
